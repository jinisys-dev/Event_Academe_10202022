/*!50003 DROP PROCEDURE IF EXISTS `spReportMonthlySalesSummaryAllTransactionCodes` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportMonthlySalesSummaryAllTransactionCodes`(
    IN pHotelid INT(5),
    IN pMonth INT,
    IN pYear int
    )
BEGIN
SELECT transactioncode, SUM(baseamount) AS BaseAmount, SUM(grossAmount) AS GrossAmount, SUM(netAmount) AS NetAmount, SUM(netbaseamount) AS NetBaseAmount, SUM(discount) AS Discount, SUM(govttax) AS GovtTax, SUM(mealamount) AS MealAmount, SUM(localtax) AS LocalTax, SUM(servicecharge) AS ServiceCharge, SUM(transactioncount) AS TransactionCount, TransactionDescription, acctside, SUM(monthtodate) AS MonthToDate, SUM(yeartodate) AS YearToDate FROM
(
	SELECT
	transactioncode.tranCode AS transactionCode,
	SUM(IF (month(foliotransactions.transactionDate) = pMonth and year(foliotransactions.TransactionDate) = pYear,
		IF(foliotransactions.acctside = "CREDIT", foliotransactions.baseAmount * -1, foliotransactions.baseAmount), 0)
		) AS BaseAmount,
	SUM(IF (MONTH(foliotransactions.transactionDate) = pMonth AND YEAR(foliotransactions.TransactionDate) = pYear,
		IF(foliotransactions.acctside = "CREDIT", foliotransactions.grossAmount * -1, foliotransactions.grossAmount), 0)
		) AS GrossAmount,
	SUM(IF (MONTH(foliotransactions.transactionDate) = pMonth AND YEAR(foliotransactions.TransactionDate) = pYear,
		IF(foliotransactions.acctside = "CREDIT", foliotransactions.netAmount * -1, foliotransactions.netAmount), 0)
		) AS NetAmount,
	SUM(IF (MONTH(foliotransactions.transactionDate) = pMonth AND YEAR(foliotransactions.TransactionDate) = pYear,
		IF(foliotransactions.acctside = "CREDIT", foliotransactions.netBaseAmount * -1, foliotransactions.netBaseAmount), 0)
		) AS NetBaseAmount,
	SUM(IF (MONTH(foliotransactions.transactionDate) = pMonth AND YEAR(foliotransactions.TransactionDate) = pYear,
		IF(foliotransactions.acctside = "CREDIT", foliotransactions.discount * -1, foliotransactions.discount), 0)
		) AS Discount,
	SUM(IF (MONTH(foliotransactions.transactionDate) = pMonth AND YEAR(foliotransactions.TransactionDate) = pYear,
		IF(foliotransactions.acctside = "CREDIT", foliotransactions.governmentTax * -1, foliotransactions.governmentTax), 0)
		) AS GovtTax,
	SUM(IF (MONTH(foliotransactions.transactionDate) = pMonth AND YEAR(foliotransactions.TransactionDate) = pYear,
		IF(foliotransactions.acctside = "CREDIT", foliotransactions.mealAmount * -1, foliotransactions.mealAmount), 0)
		) AS MealAmount,
	SUM(IF (MONTH(foliotransactions.transactionDate) = pMonth AND YEAR(foliotransactions.TransactionDate) = pYear,
		IF(foliotransactions.acctside = "CREDIT", foliotransactions.localtax * -1, foliotransactions.localtax), 0)
		) AS LocalTax,
	SUM(IF (MONTH(foliotransactions.transactionDate) = pMonth AND YEAR(foliotransactions.TransactionDate) = pYear,
		IF(foliotransactions.acctside = "CREDIT", foliotransactions.servicecharge * -1, foliotransactions.servicecharge), 0)
		) AS ServiceCharge,
	SUM(IF (MONTH(foliotransactions.transactionDate) = pMonth AND YEAR(foliotransactions.TransactionDate) = pYear,1,0)) AS TransactionCount,
	transactioncode.memo AS TransactionDescription,
	transactioncode.`AcctSide`,
	SUM(IF (MONTH(foliotransactions.transactionDate) = MONTH(pMonth) AND month(foliotransactions.transactiondate) <= pMonth AND YEAR(foliotransactions.transactiondate)=YEAR(pYear), 
		IF(foliotransactions.acctSide = "CREDIT", foliotransactions.netBaseAmount * -1, foliotransactions.netBaseAmount), 0)
	) AS MonthToDate,
	SUM(IF (YEAR(foliotransactions.transactiondate)=YEAR(pYear) AND year(foliotransactions.transactiondate) <= pYear, 
		IF(foliotransactions.acctSide = "CREDIT", foliotransactions.netBaseAmount * -1, foliotransactions.netBaseAmount), 0)
	) AS YearToDate
	FROM
	transactioncode
	LEFT JOIN
	foliotransactions
	ON
	transactioncode.tranCode = foliotransactions.transactionCode
	WHERE
	transactioncode.StateFlag = 'ACTIVE'
	GROUP BY 
	transactioncode.tranCode
	UNION ALL
	SELECT
	transactioncode.tranCode AS transactionCode,
	SUM(IF (month(nonguesttransaction.transactionDate) = pMonth AND YEAR(nonguesttransaction.TransactionDate) = pYear,
		IF(nonguesttransaction.acctside = "CREDIT", nonguesttransaction.baseAmount * -1, nonguesttransaction.baseAmount), 0)
		) AS BaseAmount,
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = pMonth AND YEAR(nonguesttransaction.TransactionDate) = pYear,
		IF(nonguesttransaction.acctside = "CREDIT", nonguesttransaction.grossAmount * -1, nonguesttransaction.grossAmount), 0)
		) AS GrossAmount,
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = pMonth AND YEAR(nonguesttransaction.TransactionDate) = pYear,
		IF(nonguesttransaction.acctside = "CREDIT", nonguesttransaction.netAmount * -1, nonguesttransaction.netAmount), 0)
		) AS NetAmount,
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = pMonth AND YEAR(nonguesttransaction.TransactionDate) = pYear,
		IF(nonguesttransaction.acctside = "CREDIT", nonguesttransaction.netBaseAmount * -1, nonguesttransaction.netBaseAmount), 0)
		) AS NetBaseAmount,
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = pMonth AND YEAR(nonguesttransaction.TransactionDate) = pYear,
		IF(nonguesttransaction.acctside = "CREDIT", nonguesttransaction.discount * -1, nonguesttransaction.discount), 0)
		) AS Discount,
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = pMonth AND YEAR(nonguesttransaction.TransactionDate) = pYear,
		IF(nonguesttransaction.acctside = "CREDIT", nonguesttransaction.governmentTax * -1, nonguesttransaction.governmentTax), 0)
		) AS GovtTax,
	0 AS MealAmount,
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = pMonth AND YEAR(nonguesttransaction.TransactionDate) = pYear,
		IF(nonguesttransaction.acctside = "CREDIT", nonguesttransaction.localtax * -1, nonguesttransaction.localtax), 0)
		) AS LocalTax,
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = pMonth AND YEAR(nonguesttransaction.TransactionDate) = pYear,
		IF(nonguesttransaction.acctside = "CREDIT", nonguesttransaction.servicecharge * -1, nonguesttransaction.servicecharge), 0)
		) AS ServiceCharge,
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = pMonth AND YEAR(nonguesttransaction.TransactionDate) = pYear,1,0)) AS TransactionCount,
	transactioncode.memo AS TransactionDescription,
	transactioncode.`AcctSide`,
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = MONTH(pMonth) AND month(nonguesttransaction.transactiondate) <= pMonth AND YEAR(nonguesttransaction.transactiondate)=YEAR(pYear), 
		IF(nonguesttransaction.acctSide = "CREDIT", nonguesttransaction.netBaseAmount * -1, nonguesttransaction.netBaseAmount), 0)
	) AS MonthToDate,
	SUM(IF (YEAR(nonguesttransaction.transactiondate)=YEAR(pYear) AND year(nonguesttransaction.transactiondate) <= pYear, 
		IF(nonguesttransaction.acctSide = "CREDIT", nonguesttransaction.netBaseAmount * -1, nonguesttransaction.netBaseAmount), 0)
	) AS YearToDate
	FROM
	transactioncode
	LEFT JOIN
	nonguesttransaction
	ON
	transactioncode.tranCode = nonguesttransaction.transactionCode
	WHERE
	transactioncode.StateFlag = 'ACTIVE'
	GROUP BY 
	transactioncode.tranCode
) AS transactionTable GROUP BY transactioncode;
    END */;;
