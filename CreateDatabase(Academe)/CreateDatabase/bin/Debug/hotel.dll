/*!50003 DROP PROCEDURE IF EXISTS `spReportBInvoiceSched` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportBInvoiceSched`(
in pFolioId varchar(20),
in pHotelId int(5)
)
BEGIN
Select f.Remarks into @remarks from folio f where f.folioid = pFolioId;
SELECT GROUP_CONCAT(folio.`FolioId`) into @mFolios FROM folio WHERE folio.`MasterFolio` = pFolioId and folio.`HotelId` = pHotelId;
SELECT
  f.`FolioId`,
  f.`AccountType`, 
  f.`FolioType`,
  IF(
    f.`AccountId` = '',
    
    IF(
      INSTR(f.CompanyId, 'I-'),
	CONCAT(g.`Title`,' ',g.`FirstName`,' ',g.`LastName`),
	c.`CompanyName`),
    
    CONCAT(g.`Title`,' ',g.`FirstName`,' ',g.`LastName`) ) AS guestName,
  IF(f.`AccountId` = '',
     
     IF(
     INSTR(f.CompanyId, 'I-'), 
        CONCAT(g.TelephoneNo, ' / ', g.MobileNo),
        CONCAT(c.ContactNumber1, ' / ', c.ContactNumber3)),
      CONCAT(g.TelephoneNo, ' ', g.MobileNo) ) AS ContactNo,
  IF(f.`AccountId` = '',
     
     IF(
     INSTR(f.CompanyId, 'I-'), 
        g.`EmailAddress`,
        c.`Email1`),
      g.`EmailAddress` ) AS EmailAd,
  IF(
    f.`AccountId` = '',
    
    IF(
      INSTR(f.CompanyId, 'I-'),
	g.Country,
	c.`Country1`),
    
    g.Country ) AS Country,
  fs.`RoomType`,
  fs.`RoomId`,
  f.`ArrivalDate`,
  f.`DepartureDate`,
  fs.`Days`,
  @remarks as `Remarks`
FROM
  folio f 
  LEFT JOIN guests g 
    ON f.`AccountId` = g.`AccountId` 
    OR f.`CompanyId` = g.`AccountId`
  LEFT JOIN company c 
    ON f.`CompanyId` = c.`CompanyId` 
  LEFT JOIN folioschedules fs 
    ON f.`FolioId` = fs.`FolioId` 
WHERE f.folioid = pFolioId
AND  f.`HotelId` = pHotelId
UNION ALL
SELECT
  f.`FolioId`,
  f.`AccountType`,
  f.`FolioType`,
  CONCAT(g.`Title`,' ',g.`FirstName`,' ',g.`LastName`) AS guestName,
  CONCAT(g.TelephoneNo, ' / ', g.MobileNo) AS ContactNo,
  g.`EmailAddress` AS EmailAd,  
  g.Country,
  ifnull(fs.`RoomType`, (select RoomType from folioschedules where folioschedules.`FolioId` = f.`MasterFolio`)) as RoomType,
  IFNULL(fs.`RoomId`, (Select RoomId from folioschedules where folioschedules.`FolioId` = f.`MasterFolio`)) as RoomId,
  f.`ArrivalDate`,
  f.`DepartureDate`,
  ifnull(fs.`Days`, (SELECT Days FROM folioschedules WHERE folioschedules.`FolioId` = f.`MasterFolio`)) as Days,
  @remarks AS `Remarks`
FROM
  folio f 
  LEFT JOIN guests g 
    ON f.`AccountId` = g.`AccountId` 
  LEFT JOIN folioschedules fs 
    ON f.`FolioId` = fs.`FolioId` 
WHERE (f.`MasterFolio` = pFolioId
or (instr(@mFolios, f.`MasterFolio`) and f.`MasterFolio` != ''))
AND f.`Status` not in ('CANCELLED', 'REMOVED')
and  f.`HotelId` = pHotelId;
END */;;
