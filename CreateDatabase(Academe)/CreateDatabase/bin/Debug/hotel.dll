/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`%`*/ /*!50003 PROCEDURE `spSAP_InsertBusinessPartner`(
	pHotelId int(11)
	, pAccountId varchar(30)
	, pAccountName varchar(30)
	, pCompanyId varchar(30)
	, pCompanyName varchar(30)
	, pDetailed tinyint(1)
	, pBPCode varchar(30)
	, pUser varchar(30)
)
BEGIN
	set @account = 0;
	
	select count(*) into @account 
		from sap_businesspartners 
		where sap_businesspartners.HotelId = pHotelId
			and sap_businesspartners.AccountId = pAccountId limit 1;
	if (@account > 0) then
		update sap_businesspartners
			set sap_businesspartners.AccountName = pAccountName
				, sap_businesspartners.CompanyId = pCompanyId
				, sap_businesspartners.CompanyName = pCompanyName
				, sap_businesspartners.Detailed = pDetailed
				, sap_businesspartners.BPCode = pBPCode
				, sap_businesspartners.UpdatedBy = pUser
				, sap_businesspartners.UpdatedOn = now()
			where sap_businesspartners.HotelId = pHotelId
				and sap_businesspartners.AccountId = pAccountId;
	else
		insert into sap_businesspartners
			set sap_businesspartners.HotelId = pHotelId
			, sap_businesspartners.AccountId = pAccountId
			, sap_businesspartners.AccountName = pAccountName
			, sap_businesspartners.CompanyId = pCompanyId
			, sap_businesspartners.CompanyName = pCompanyName
			, sap_businesspartners.Detailed = pDetailed
			, sap_businesspartners.BPCode = pBPCode
			, sap_businesspartners.CreatedOn = now()
			, sap_businesspartners.CreatedBy = pUser
			, sap_businesspartners.UpdatedOn = now()
			, sap_businesspartners.UpdatedBy = pUser;
	end if;
    END */;;
