/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`%`*/ /*!50003 PROCEDURE `spSAP_GetUnpostedDailyHotelTransactions`(
	in pHotelId int(4),
	in pStartDate date,
	in pEndDate date
	)
BEGIN
	select
	foliotransactions.FolioTransactionNo,
	0 as NonGuestTransactionId,
	foliotransactions.TransactionCode,
	if(folio.foliotype='GROUP' OR foliotransactions.SubFolio = 'B', folio.companyid, folio.accountid) as AccountId,
	
	transactioncode.Memo,
	foliotransactions.AcctSide,
	if(foliotransactions.AcctSide = 'CREDIT', foliotransactions.BaseAmount * -1, foliotransactions.BaseAmount) as BaseAmount,
	
	if(foliotransactions.AcctSide = 'CREDIT', if(transactioncode.`AcctSide` = 'DEBIT', foliotransactions.`NetBaseAmount` * -1, foliotransactions.NetAmount * -1), foliotransactions.NetAmount) as NetAmount,
	
	if(foliotransactions.AcctSide = 'CREDIT', if(transactioncode.`AcctSide` = 'DEBIT', foliotransactions.`NetAmount` * -1, (foliotransactions.`NetBaseAmount`) * -1), (foliotransactions.`NetBaseAmount`)) as NetBaseAmount,
	if(foliotransactions.AcctSide = 'CREDIT', foliotransactions.ServiceCharge * -1, foliotransactions.ServiceCharge) as ServiceCharge,
	if(foliotransactions.AcctSide = 'CREDIT', foliotransactions.Discount * -1, foliotransactions.Discount) as Discount,
	foliotransactions.ServiceChargeInclusive,
	if(foliotransactions.AcctSide = 'CREDIT', foliotransactions.GovernmentTax * -1, foliotransactions.GovernmentTax) as GovernmentTax,
	foliotransactions.GovernmentTaxInclusive as GovtTaxInclusive,
	if(foliotransactions.AcctSide = 'CREDIT', foliotransactions.LocalTax * -1, foliotransactions.LocalTax) as LocalTax,
	foliotransactions.LocalTaxInclusive,
	if(foliotransactions.AcctSide = 'CREDIT', foliotransactions.MealAmount * -1, foliotransactions.MealAmount) as MealAmount,
	foliotransactions.TransactionDate,
	foliotransactions.BankName
	from
	foliotransactions
	left join
	transactioncode
	on
	foliotransactions.TransactionCode = transactioncode.TranCode
	left join
	folio
	on
	foliotransactions.folioId = folio.folioID
	where
	date(foliotransactions.TransactionDate) >= pStartDate and 
	date(foliotransactions.TransactionDate) <= pEndDate and
	
	foliotransactions.PostToAcctg = 0 AND
	foliotransactions.HotelID = pHotelId
	and
	foliotransactions.status != 'VOID' and
	foliotransactions.`TransactionCode` != 4200
	and foliotransactions.`TransactionCode` != 1201
	and foliotransactions.`NetAmount` != 0
	and foliotransactions.`TransactionCode` != 4100
	
	
	union all
	select
	0 as FolioTransactionNo,
	nonguesttransaction.TransactionId as NonGuestTransactionId,
	nonguesttransaction.TransactionCode,
	nonguesttransaction.AccountId,
	transactioncode.Memo,
	nonguesttransaction.AcctSide,
	
	if(nonguesttransaction.AcctSide = 'CREDIT', nonguesttransaction.BaseAmount * -1, nonguesttransaction.BaseAmount) as BaseAmount,
	
	
	if(nonguesttransaction.AcctSide = 'CREDIT', if(transactioncode.`AcctSide` = 'DEBIT', (nonguesttransaction.NetAmount - nonguesttransaction.ServiceCharge - nonguesttransaction.GovernmentTax - nonguesttransaction.LocalTax) * -1, nonguesttransaction.NetAmount * -1), nonguesttransaction.NetAmount) as NetAmount,
	
	
	IF(nonguesttransaction.AcctSide = 'CREDIT', if(transactioncode.`AcctSide` = 'DEBIT', nonguesttransaction.`NetAmount` * -1, (nonguesttransaction.NetAmount - nonguesttransaction.ServiceCharge - nonguesttransaction.GovernmentTax - nonguesttransaction.LocalTax) * -1), (nonguesttransaction.NetAmount - nonguesttransaction.ServiceCharge - nonguesttransaction.GovernmentTax - nonguesttransaction.LocalTax)) AS NetBaseAmount,
	if(nonguesttransaction.AcctSide = 'CREDIT', nonguesttransaction.ServiceCharge * -1, nonguesttransaction.ServiceCharge) as ServiceCharge,
	if(nonguesttransaction.AcctSide = 'CREDIT', nonguesttransaction.Discount * -1, nonguesttransaction.Discount) as Discount,
	nonguesttransaction.ServiceChargeInclusive,
	if(nonguesttransaction.AcctSide = 'CREDIT', nonguesttransaction.GovernmentTax * -1, nonguesttransaction.GovernmentTax) as GovernmentTax,
	nonguesttransaction.GovernmentTaxInclusive as GovtTaxInclusive,
	if(nonguesttransaction.AcctSide = 'CREDIT', nonguesttransaction.LocalTax * -1, nonguesttransaction.LocalTax) as LocalTax,
	nonguesttransaction.LocalTaxInclusive,
	0 as MealAmount,
	nonguesttransaction.TransactionDate,
	nonguesttransaction.BankName
	from
	nonguesttransaction
	left join
	transactioncode
	on
	nonguesttransaction.TransactionCode = transactioncode.TranCode
	where
	date(nonguesttransaction.TransactionDate) >= pStartDate and 
	date(nonguesttransaction.TransactionDate) <= pEndDate and
	
	nonguesttransaction.PostToAcctg = 0 and
	nonguesttransaction.HotelID = pHotelId and
	nonguesttransaction.statusFlag != 'VOID' and
	nonguesttransaction.`TransactionCode` != 4200
	and nonguesttransaction.`TransactionCode` != 1201
	
	
	;
    END */;;
