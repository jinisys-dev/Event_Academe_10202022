/*!50003 DROP PROCEDURE IF EXISTS `spUpdateElectricMeter` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spUpdateElectricMeter`(
  IN pMeterId VARCHAR (20),
  IN pRoomId VARCHAR (10),
  IN pNPwrRate DECIMAL (12, 2),
  IN pEPwrRate DECIMAL (12, 2)
)
BEGIN
  (SELECT 
    COUNT(RoomId) into @cnt 
  FROM
    `electricmeters` 
  WHERE RoomId = pRoomId);
  if @cnt = 0 
  then SET @sqlQry = CONCAT(
    "INSERT INTO `electricmeters` 
(MeterId, RoomId, NormalPowerRate, EmergencyPowerRate)
VALUES
(",
    pMeterId,
    ", ",
    pRoomId,
    ", ",
    pNPwrRate,
    ", ",
    pEPwrRate,
    ");"
  ) ;
  else
  SET @sqlQry = CONCAT(
    "UPDATE `electricmeters` 
SET MeterId = ",
    pMeterId,
    ",
NormalPowerRate = ",
    pNPwrRate,
    ",
EmergencyPowerRate = ",
    pEPwrRate,
    "
WHERE 
RoomId = ",
    pRoomId,
    ";"
  ) ;
  end if;
  
  PREPARE STMT FROM @sqlQry ;
  EXECUTE STMT ;
  DEALLOCATE PREPARE STMT ;
END */;;
