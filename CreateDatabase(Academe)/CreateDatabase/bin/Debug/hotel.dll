/*!50003 DROP FUNCTION IF EXISTS `fGetRoomOccupancyPerDay` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 FUNCTION `fGetRoomOccupancyPerDay`(pStartDate DATE,
    pEndDate DATE
    ) RETURNS int(11)
BEGIN
	DECLARE done INT DEFAULT 0;
	DECLARE currDate DATE;
	DECLARE totalOccupancy INT;
	DECLARE cur1 CURSOR FOR SELECT DISTINCT eventdate FROM roomevents WHERE eventdate <= pEndDate AND eventdate >= pStartDate;
	DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = 1;	
	SET totalOccupancy = 0;
	OPEN cur1;
	read_loop: LOOP
	   FETCH cur1 INTO currDate;
	   IF done = 1 THEN
		LEAVE read_loop;
	   END IF;
		SELECT (totalOccupancy + COUNT(roomevents.RoomID)) INTO totalOccupancy
		FROM roomevents 
		LEFT JOIN folio ON roomevents.`EventID` = folio.`folioID`
		WHERE DATE(eventdate) = DATE(currDate) 
		AND roomevents.eventtype NOT IN ('CANCELLED') 
		AND roomevents.roomid NOT IN (SELECT roomid FROM rooms WHERE roomtypecode = 'FUNCTION')
		AND roomevents.transferflag = 0
		AND ( DATE(folio.fromDate) <= DATE(currDate) AND DATE(folio.toDate) > DATE(currDate));
	   
	END LOOP;
	CLOSE cur1;
	RETURN totalOccupancy;
    END */;;
