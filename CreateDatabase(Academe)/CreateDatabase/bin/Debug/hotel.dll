/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spSelectCurrentDayRoomStatus`(
in pDate date,
in pHotelID int(5)
)
BEGIN
set sql_big_selects = 1;
select 
roomid,
roomtypecode,
cleaningstatus,
eventtype
From
(
select distinct
rooms.roomid,
if(length(rooms.floor) < 2, concat("0",rooms.floor), rooms.floor) as `floor`,
rooms.roomtypecode,
rooms.cleaningstatus,
roomevents.eventtype 
from rooms use index(roomid)
left join roomevents use index(roomid,eventtype,eventdate) on 
rooms.roomid = roomevents.roomid and 
roomevents.hotelid=pHotelID and
roomevents.EventDate= pDate and 
roomevents.eventtype<>'CHECKED OUT'  and
roomevents.eventtype <> 'NO SHOW' and 
roomevents.eventtype <> 'CANCELLED' and
roomevents.transferFlag <>'1'
where
rooms.stateFlag != 'DELETED'
) as table1 
order by table1.Floor, table1.roomid;
END */;;
/*!50003 SET SESSION SQL_MODE=@OLD_SQL_MODE*/;;
/*!50003 DROP PROCEDURE IF EXISTS `spSelectCurrentDayRoomStatusPagination` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spSelectCurrentDayRoomStatusPagination`(
in pDate date,
in pHotelID int(5),
pRoomType varchar(50),
pOffset int,
pLimit int
)
BEGIN
set sql_big_selects = 1;

SET @SQL = CONCAT("select 
roomid,
roomtypecode,
cleaningstatus,
eventtype
From
(select distinct
rooms.roomid as roomid,
if(length(rooms.floor) < 2, concat('0',rooms.floor), rooms.floor) as `floor`,
rooms.roomtypecode,
rooms.cleaningstatus,
fGetCurrentRoomEvent(rooms.RoomId,'",pDate,"') as eventtype
from rooms left join roomevents 
on rooms.roomid = roomevents.roomid and 
roomevents.hotelid=  ",pHotelID,"  and
roomevents.EventDate= date('",pDate,"') and 
roomevents.eventtype<>'CHECKED OUT'  and
roomevents.eventtype <> 'NO SHOW' and 
roomevents.eventtype <> 'CANCELLED' and
roomevents.transferFlag <>'1'
and if(fCountRoomInHouse('",pDate,"','",pHotelID,"','",pRoomType,"',rooms.RoomId)>1,roomevents.EventType=fGetCurrentRoomEvent(rooms.RoomId,'",pDate,"'),true)
where
rooms.stateFlag != 'DELETED'
and rooms.StateFlag != 'INACTIVE'
and 
",IF(pRoomType <> 'BED' AND pRoomType <> 'FUNCTION',"rooms.RoomTypeCode <> 'BED' and rooms.RoomTypeCode <> 'FUNCTION'", CONCAT("rooms.RoomTypeCode = '",pRoomType, "'")),"
) as table1 
order by table1.Floor, table1.roomid
LIMIT ",pOffset, ", ", pLimit);
PREPARE STMT FROM @SQL;
EXECUTE STMT;
DEALLOCATE PREPARE STMT;
END */;;
