/*!50003 DROP PROCEDURE IF EXISTS `spReportDotSummary` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportDotSummary`(
pDate DATE
)
BEGIN
SET @prevDate := CAST((SELECT MIN(auditdate) FROM auditdatetable WHERE  EXTRACT(YEAR_MONTH FROM auditdate) = EXTRACT(YEAR_MONTH FROM pDate)) AS DATE);
SET @rCount := FGetTotalRoomsOnly();
SET @prevGuestCount := IFNULL(IFNULL(fGetPreviousGuestCount(CAST((SELECT LAST_DAY(pDate - INTERVAL 1 MONTH)) AS DATE)), 
	IF(EXTRACT(DAY FROM @prevDate) = 1, fGetPreviousGuestCount(CAST((SELECT LAST_DAY(pDate - INTERVAL 1 MONTH)) AS DATE)), fGetPreviousGuestCount(@prevDate) )), 0);
SET @counter := 0;
SET @prevGuestNight := 0;
SELECT id, `date`, `day`, dailyGuestArrival, dailyGuestDeparture, guestNight, occupiedRoom, occupancyRate,      
       ROUND(guestNight / occupiedRoom, 2) AS guestPerRoom,
       @rCount AS roomCount,
       @prevGuestCount AS prevGuest
FROM
(
SELECT 
id, 
`date`,
`day`,
dailyGuestArrival,
dailyGuestDeparture,
IF(id = 1, @prevGuestNight := @prevGuestCount + dailyGuestArrival - dailyGuestDeparture, 
	@prevGuestNight := @prevGuestNight + dailyGuestArrival - dailyGuestDeparture) AS guestNight,
occupiedRoom,
ROUND(occupiedRoom / @rCount * 100, 2) AS occupancyRate,
0 AS guestPerRoom
FROM
(
SELECT 
   @counter := @counter + 1 AS id,
   DAY(auditdate) AS `date`,
   SUBSTRING(DAYNAME(auditdate), 1, 3) AS `day`,
   IFNULL(fGetDailyGuestArrival(auditdate), 0) AS dailyGuestArrival,
   IFNULL(fGetDailyGuestDeparture(auditdate), 0) AS dailyGuestDeparture,
   0 AS guestNight,
   IFNULL(fGetOccupiedRoomCount(auditdate), 0) AS occupiedRoom,
   0 AS occupancyRate,
   0 AS guestPerRoom
FROM
   auditdatetable
WHERE 
   EXTRACT(YEAR_MONTH FROM auditdate) = EXTRACT(YEAR_MONTH FROM pDate)
   AND Meridian = 'PROCESSED') AS sql1 ) AS sql2;
END */;;
