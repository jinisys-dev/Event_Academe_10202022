/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportDotNationality`(
pDate DATE
)
BEGIN
SET @counter := 2;
SELECT 
    1 AS id,
   'Filipinos' AS Citizenship,
    fGetFilipinoCount(pDate, 'FILIPINO') AS `count`
    
UNION ALL 
SELECT 
    2 AS id,
    'Total Foreign Arrivals' AS Citizenship,
    fGetFilipinoCount(pDate, '*') AS `count`
UNION ALL 
SELECT
    @counter := @counter + 1 AS id,
    if(guests.`Citizenship` = '', 'OTHERS', guests.`Citizenship`), 
    COUNT(DISTINCT folioid) AS `count`
  FROM
