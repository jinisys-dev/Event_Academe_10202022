/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spRoomOccupancyReport`(
pAuditDate DATE
)
BEGIN

SELECT 
DISTINCT
fGetActualRoomOccupied(folio.folioid,pAuditDate) AS roomid,
rooms.roomtypecode,
CONCAT(guests.`Title`, ' ', guests.lastName, ", ",guests.FirstName) AS GuestName,
folio.fromDate AS ArrivalDate,
folio.toDate AS DepartureDate,
folio.noofadults,
company.companyName,
folio.remarks,
FGetTotalRoomsOnly() AS totalRoomCount,
fGetTotalOutOfOrderRoomsOnly(pAuditDate) AS OutOfOrderRooms
FROM 
rooms
LEFT JOIN roomevents ON roomevents.roomid = rooms.roomid
LEFT JOIN folio ON folio.folioid = roomevents.eventid
LEFT JOIN guests ON guests.accountid = folio.accountid
LEFT JOIN company ON company.companyId = folio.companyId
LEFT JOIN folioschedules ON folioschedules.folioid = folio.folioid 
AND (pAuditDate BETWEEN DATE(folioschedules.`FromDate`) AND DATE(folioschedules.`ToDate`)) 
WHERE
DATE(roomevents.eventdate) = pAuditDate AND roomevents.transferflag = 0 AND
IF(DATE(pAuditDate) = (SELECT auditdate FROM auditdatetable WHERE Meridian != 'PROCESSED'),
roomevents.EventType = 'IN HOUSE' 
,roomevents.EventType IN ('CHECKED OUT', 'IN HOUSE') AND DATE(folio.DepartureDate) != pAuditDate) 
AND (rooms.roomtypecode != 'FUNCTION' AND rooms.roomtypecode != 'BED')
GROUP BY folioschedules.folioid
ORDER BY rooms.roomid;
END */;;
