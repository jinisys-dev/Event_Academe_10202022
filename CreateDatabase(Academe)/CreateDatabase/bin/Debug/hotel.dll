/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`%`*/ /*!50003 PROCEDURE `spReportAnnualRevenueSummary`(
	IN pTranDate DATE,
	IN pHotelId INT(5)
	)
BEGIN
SELECT transactioncode, PreBTrans, TransactionDescription, 
SUM(January) AS January, 
SUM(February) AS February, 
SUM(March) AS March, 
SUM(April) AS April, 
SUM(May) AS May, 
SUM(June) AS June, 
SUM(July) AS July, 
SUM(August) AS August, 
SUM(September) AS September, 
SUM(October) AS October, 
SUM(November) AS November, 
SUM(December) AS December
 FROM
(
	SELECT
	transactioncode.tranCode AS transactionCode,
	
	tb1.PBTrans as PreBTrans,
	transactioncode.memo AS TransactionDescription,
	
	SUM(IF (MONTH(foliotransactions.transactionDate) = 1  AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS January,
			
	SUM(IF (MONTH(foliotransactions.transactionDate) = 2  AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS February,
	
	SUM(IF (MONTH(foliotransactions.transactionDate) = 3  AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS March,
			
	SUM(IF (MONTH(foliotransactions.transactionDate) = 4  AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS April,
			
	SUM(IF (MONTH(foliotransactions.transactionDate) = 5  AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS May,	
		
	SUM(IF (MONTH(foliotransactions.transactionDate) = 6  AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS June,	
		
	SUM(IF (MONTH(foliotransactions.transactionDate) = 7  AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS July,	
		
	SUM(IF (MONTH(foliotransactions.transactionDate) = 8  AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS August,
			
	SUM(IF (MONTH(foliotransactions.transactionDate) = 9  AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS September,
			
	SUM(IF (MONTH(foliotransactions.transactionDate) = 10 AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS October,	
		
	SUM(IF (MONTH(foliotransactions.transactionDate) = 11 AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS November,
			
	SUM(IF (MONTH(foliotransactions.transactionDate) = 12 AND YEAR(foliotransactions.transactiondate)=YEAR(pTranDate), 
		foliotransactions.netBaseAmount, 0)
	) AS December
	
	FROM
	(SELECT 0 AS PBTrans
	UNION ALL SELECT 1 AS PBTrans) tb1
	JOIN 
	transactioncode
	on
	transactioncode.StateFlag = 'ACTIVE'
	AND transactioncode.`tranTypeId` = 1
	LEFT JOIN
	foliotransactions
	ON
	transactioncode.tranCode = foliotransactions.transactionCode
	and 
	tb1.PBTrans = foliotransactions.`PreBookingTrans`
	AND foliotransactions.`Status` = 'ACTIVE'
	GROUP BY 
	transactioncode.tranCode, `PBTrans`
	UNION ALL
	SELECT
	transactioncode.tranCode AS transactionCode,
	0 AS PreBTrans,
	transactioncode.memo AS TransactionDescription,
	
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 1  AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS January,
			
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 2  AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS February,
		
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 3  AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS March,
	
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 4  AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS April,	
		
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 5  AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS May,	
		
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 6  AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS June,	
		
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 7  AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS July,
	
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 8  AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS August,
	
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 9  AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS September,	
		
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 10 AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS October,	
		
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 11 AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS November,
		
	SUM(IF (MONTH(nonguesttransaction.transactionDate) = 12 AND YEAR(nonguesttransaction.transactiondate)=YEAR(pTranDate), 
		nonguesttransaction.netBaseAmount, 0)
	) AS December
	
	FROM	
	transactioncode
	LEFT JOIN
	nonguesttransaction
	ON
	transactioncode.tranCode = nonguesttransaction.transactionCode
	WHERE
	transactioncode.StateFlag = 'ACTIVE'
	AND transactioncode.`tranTypeId` = 1
	and nonguesttransaction.`StatusFlag` = 'ACTIVE'
	GROUP BY 
	transactioncode.tranCode
) AS transactionTable GROUP BY transactioncode, PreBTrans
ORDER BY PreBTrans, transactioncode;
    END */;;
