/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`%`*/ /*!50003 PROCEDURE `spCountRooms`(
in pHotelId int(5),
in pRoomType varchar(50)
	)
BEGIN
	SET @SQL = concat("select count(RoomId) from rooms");
	IF(pRoomType <> 'FUNCTION' AND pRoomType <> 'BED') THEN
		SET @SQL= concat(@SQL," where RoomTypeCode <> 'FUNCTION' and RoomTypeCode <> 'BED'");
	ELSE
		SET @SQL = CONCAT(@SQL," where RoomTypeCode = '",pRoomType,"'");
	END IF;
	SET @SQL = CONCAT(@SQL," and HotelId = '",pHotelId,"'and StateFlag <> 'DELETED'");
	PREPARE STMT FROM @SQL;
	EXECUTE STMT;
	DEALLOCATE PREPARE STMT;
    END */;;
