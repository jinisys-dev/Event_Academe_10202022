/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportMonthlySalesSummary`(
in pHotelid int(5),
in pMonth int,
in pYear int
)
BEGIN
(
select transactioncode, sum(baseamount) as BaseAmount, 
sum(grossAmount) as GrossAmount, 
sum(netAmount) as NetAmount, 
