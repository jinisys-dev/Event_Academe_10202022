/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`%`*/ /*!50003 PROCEDURE `spsap_getinhouseguests`(
	pHotelId int(11)
	, pTransactionDate date
)
BEGIN

	SELECT DISTINCT AccountId
	, IF(AccountId LIKE 'I-%',
		(SELECT AccountName FROM guests WHERE guests.`accountid` = dtls.AccountId), 
		(SELECT CompanyName FROM company WHERE company.`CompanyId` = dtls.AccountId)
	) AS AccountName
	FROM
	(
		(
		SELECT 
		 ft.`FolioTransactionNo`
		, if(IF(f.`FolioType` = 'GROUP' or ft.`SubFolio` = 'B', f.`CompanyId`,f.`AccountId`) = '',fGetAccountIDOfLedger(ft.folioid, ft.subfolio),IF(f.`FolioType` = 'GROUP' or ft.`SubFolio` = 'B', f.`CompanyId`,f.`AccountId`)) AS AccountId
		
		FROM foliotransactions ft
		LEFT JOIN folio f
		ON ft.`FolioId` = f.`FolioId`
		WHERE ft.`HotelId`= pHotelId 
		AND DATE(ft.`TransactionDate`) = pTransactionDate
		)
	UNION
		(SELECT
		 ngt.`TransactionId`
		, ngt.`AccountId`
		FROM nonguesttransaction ngt
		WHERE DATE(ngt.`TransactionDate`) = pTransactionDate
		AND ngt.`HotelId` = pHotelId
		)
	) dtls
	ORDER BY AccountId;
END */;;
