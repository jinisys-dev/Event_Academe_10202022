/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`%`*/ /*!50003 FUNCTION `fGetCurrentRoomEvent`(
    pRoomId varchar(15),
    pAuditDate Date
    ) RETURNS varchar(25) CHARSET latin1
BEGIN
    return(
	select EventType from(
		(
		select EventType, StartDate
		from
			(SELECT EventType, (IF(LEFT(EventId,1) = 'F', (SELECT DATE(FromDate) FROM folioschedules WHERE FolioID = EventId AND folioschedules.`RoomId` = RoomId AND pAuditDate BETWEEN DATE(FromDate) AND DATE(ToDate) ORDER BY FromDate LIMIT 1), (SELECT StartDate FROM engineeringjobs WHERE EnggJobNo = EventId))) AS StartDate 
			FROM roomevents 
			WHERE roomid = pRoomId
			AND EventDate = pAuditDate
			AND EventType NOT IN ('CANCELLED','CHECKED OUT','NO SHOW')
			and transferFlag != 1) tbl2
			
		)
		union all
		(
		SELECT DISTINCT 'BLOCKING' AS EventType, BlockFrom as StartDate
		    FROM blockingdetails
		    WHERE RoomId = pRoomId
		    and pAuditDate BETWEEN BlockFrom AND BlockTo
		    )
		) tbl1
		ORDER BY StartDate limit 1
	);
    END */;;
