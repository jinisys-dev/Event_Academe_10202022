/*!50003 DROP PROCEDURE IF EXISTS `spGetRoomCountPerStatusAndEvent` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spGetRoomCountPerStatusAndEvent`(
    pAuditDate date
    )
BEGIN
	
	SELECT
		ifnull(if(StartDate = pAuditDate and StateFlag = 'VACANT' and CleaningStatus = 'DIRTY', '',EventType),'') as EventType, StateFlag, CleaningStatus, COUNT(*) AS Total	
	FROM rooms
	LEFT JOIN
	(
		
		
		SELECT * FROM
		(
			
			
			SELECT * FROM 
			(
			        
			        
			        
			        
			        
				SELECT roomevents.RoomId, EventType, (IF(LEFT(EventId,1) = 'F', (SELECT DATE(FromDate) FROM folioschedules WHERE FolioID = EventId AND folioschedules.`RoomId` = RoomId and pAuditDate BETWEEN date(FromDate) AND date(ToDate) ORDER BY FromDate limit 1), (SELECT StartDate FROM engineeringjobs WHERE EnggJobNo = EventId))) AS StartDate
				FROM roomevents
				WHERE
				EventDate = pAuditDate
				AND EventType IN ('IN HOUSE','ENGINEERING JOB','RESERVATION')
				and roomevents.`TransferFlag` <> 1
				UNION
				
				
				SELECT distinct RoomId, 'BLOCKED' AS EventType, BlockFrom
				FROM blockingdetails
				WHERE
				pAuditDate BETWEEN BlockFrom AND BlockTo
			) tbl1
			ORDER BY RoomId,StartDate
		) tbl2
		GROUP BY RoomId
	) tbl3
	ON rooms.`RoomId` = tbl3.RoomId
	where rooms.`RoomTypeCode` != 'FUNCTION'
	and rooms.`StateFlag` != 'DELETED'
	GROUP BY EventType, StateFlag, CleaningStatus;
    END */;;
