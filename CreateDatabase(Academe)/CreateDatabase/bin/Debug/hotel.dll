/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportDateRangeRoomInventory`(
    pStartDate date,
    pEndDate date
    )
BEGIN
	SELECT 
	rooms.`RoomTypeCode`,
	roomevents.`EventDate`,
	SUM(IF(roomevents.`EventType` = 'CHECKED OUT' OR roomevents.`EventType` = 'IN HOUSE', 1, 0)) AS Occupied,
	SUM(IF(roomevents.`EventType` = 'RESERVATION', 1, 0)) AS Reserved,
	SUM(IF(roomevents.`EventType` = 'NO SHOW', 1, 0)) AS NoShows,
	SUM(IF(roomevents.`EventType` = 'CANCELLED', 1, 0)) AS Cancelled
	FROM roomevents
	LEFT JOIN rooms
	ON roomevents.`RoomId` = rooms.`RoomId`
	LEFT JOIN folioschedules
	ON roomevents.`EventId` = folioschedules.`FolioId` AND roomevents.`RoomId` = folioschedules.`RoomId`
	WHERE roomevents.`RoomId` <> ''
	
	AND date(roomevents.`EventDate`) <> date(folioschedules.`ToDate`)
	
	AND roomevents.`EventDate` BETWEEN pStartDate AND pEndDate
	GROUP BY
	RoomTypeCode, EventDate;
    END */;;
