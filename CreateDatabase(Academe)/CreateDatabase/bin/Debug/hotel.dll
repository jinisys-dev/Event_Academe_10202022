	,' left join common_academenew.`items` itm on itm.itemid = dtl.item_id '
	
	,' left join common_academenew.`itemgroups` grp on grp.groupId = itm.groupId '
	
	,' left join resto_academenew.`payment` pmt on pmt.order_id = hdr.order_id '
	,' where '
	,' date(hdr.order_date) >= date(''',pStartDate,''') '
	,' and date(hdr.order_date) <= date(''',pEndDate,''') '
	,' and pmt.`status` != ''VOID'' '
	,' and pmt.payment_type in (''CASH'',''Credit'',''ACCOUNT_EMPLOYEE'') '
	,' and dtl.operation_status != ''CANCELLED'' '
	,' and grp.mainGroupId is not null and grp.mainGroupId != "" '
	
	,' group by '
	,' grp.mainGroupId, '
	
	,' hdr.order_id) as tbl1 '
	,' left join transactioncode tc on tbl1.transactioncode = tc.trancode '
	,' ) '
	,' UNION ALL '
	,' (select '
	,' date(tbl1.createtime), tbl1.memo, tbl1.transactioncode '
	,' , tbl1.acctside '
	,' , ',drrScript3,' '
	,' tbl1.transactionsource '
	,' , tbl1.order_id '
	,' , tbl1.customerid '
	,' , tbl1.guestname '
	,' , tbl1.netbaseamount '
	,' , 0 as `',inclusion1,'` '
	,' , 0 as `',inclusion2,'` '
	,' , 0 as `',inclusion3,'` '
	,' , tbl1.localtax '
	,' , tbl1.governmenttax '
	,' , tbl1.servicecharge '
	,' , tbl1.discount '
	,' , round(tbl1.localtax+tbl1.governmenttax+tbl1.servicecharge+tbl1.netbaseamount+tbl1.discount,4) as computedRevenue '
	,' , tbl1.netAmount as revenue '
	,' , '''' as roomid '
	,' , '''' as roomtype '
	,' , '''' as ratetype '
	,' , 0 as rate '
	,' , 0 as pax '
	,' from '
	,' (select '
	,' hdr.createtime '
	,' ,hdr.order_date '
	,' ,hdr.order_id '
	,' ,''DEBIT'' as acctSide '
	,' ,''POSCHECK#'' as TransactionSource '
	,' ,hdr.customer_id '
	,' ,concat(if(substring(hdr.customer_id,1,1) = ''F'',  '
	,'		resto_academenew.fGetHotelFolioAccountID(hdr.customer_id) '
	,'		,hdr.customer_id) '
	,'	,hdr.employee_id) as customerID '
	,' , resto_academenew.fGetCustomerNamePerOrder(hdr.Customer_ID,hdr.EMPLOYEE_ID) as GuestName '
	,' , (case fmap.description '
	,' when "OPEN FOOD" then '
	,'	(select transactioncode.TranCode  '
	,'	from transactioncode  '
	,'	where transactioncode.Memo like ''%FOOD%'' and transactioncode.AcctSide = ''DEBIT'' and transactioncode.StateFlag = ''ACTIVE'' limit 1) '
	,' when "OPEN BEVERAGE" then '
	,'	(select transactioncode.TranCode ' 
	,'	from transactioncode  '
	,'	where transactioncode.Memo like ''%BEVERAGE%'' and transactioncode.AcctSide = ''DEBIT'' and transactioncode.StateFlag = ''ACTIVE'' limit 1) '
	,' when "BANQUET FOOD" then '
	,'	(select transactioncode.TranCode  '
	,'	from transactioncode  '
	,'	where transactioncode.Memo like ''%BANQUET%'' and transactioncode.AcctSide = ''DEBIT'' and transactioncode.StateFlag = ''ACTIVE'' limit 1) '
	,' when "BANQUET BEVERAGE" then '
	,'	(select transactioncode.TranCode  '
	,'	from transactioncode  '
	,'	where transactioncode.Memo like ''%BANQUET%'' and transactioncode.AcctSide = ''DEBIT'' and transactioncode.StateFlag = ''ACTIVE'' limit 1) '
	,' else '
	,'	(select transactioncode.TranCode  '
	,'	from transactioncode  '
	,'	where transactioncode.Memo like ''%FOOD%'' and transactioncode.AcctSide = ''DEBIT'' and transactioncode.StateFlag = ''ACTIVE'' limit 1) '
	,' end) as transactionCode '
	,' , round(sum(dtl.discount+dtl.vat+dtl.local_tax+dtl.amount+dtl.service_charge),4) as baseAmount '
	,' , round(sum(dtl.amount),4) as netBaseAmount '
	,' , round(sum(dtl.discount+dtl.vat+dtl.local_tax+dtl.amount+dtl.service_charge),4) as grossAmount '
	,' , round(sum(dtl.discount),4) as discount '
	,' , round(sum(dtl.service_charge),4) as serviceCharge '
	,' , round(sum(dtl.vat),4) as governmentTax '
	,' , round(sum(dtl.local_tax),4) as localTax '
	,' , 0 as mealAmount '
	,' , concat(''RESTAURANT - DINE-IN - '',fmap.description) as `Memo` '
	,' , ''DINE-IN'' as subFolio '
	,' , round(sum(dtl.discount+dtl.vat+dtl.local_tax+dtl.amount+dtl.service_charge),4) as netAmount '
	,' , round(sum(dtl.amount),4) as Amount '
	,' , hdr.createdby '
	,' , '''' as roomid '
	,' , '''' as roomtype '
	,' , '''' as companyName '
	,' , pmt.payment_type as REMARKS '
	,' from resto_academenew.`order detail` dtl '
	,' left join resto_academenew.`order header` hdr on hdr.order_id = dtl.order_id '
	,' left join resto_academenew.`payment` pmt on pmt.order_id = hdr.order_id '
	,' left join resto_academenew.`function mapping` fmap on fmap.function_id = dtl.item_id '
	,' where '
	,' date(hdr.order_date) >= date(''',pStartDate,''') '
	,' and date(hdr.order_date) <= date(''',pEndDate,''') '
	,' and pmt.`status` != ''VOID'' '
	,' and pmt.payment_type in (''CASH'',''Credit'',''ACCOUNT_EMPLOYEE'') '
	,' and dtl.operation_status != ''CANCELLED'' '
	,' and fmap.method is not null and fmap.method != "" '
	,' group by '
	,' fmap.function_id, '
	,' hdr.order_id) as tbl1 '
	,' left join transactioncode tc on tbl1.transactioncode = tc.trancode '
	,' ) '
	,' order by `DATE`, `TRANS CODE`, `TRANS SRC`, `REF NO` '
  );
  PREPARE stmt FROM @sqlString;
  EXECUTE stmt;
  DEALLOCATE PREPARE stmt;
    END */;;
