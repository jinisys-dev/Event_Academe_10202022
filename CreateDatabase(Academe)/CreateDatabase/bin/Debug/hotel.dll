/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportRoomCleaningStatus`(
  IN pAuditDate DATE,
  IN pHotelId INT (4)
)
BEGIN
  SELECT 
    rooms.floor,
    rooms.roomid,
    rooms.roomtypecode,
    IF(
      fGetRoomEngineeringJobDescription (pAuditDate, rooms.roomid) IS NULL,
      rooms.cleaningstatus,
      IF(
        (SELECT
          EventNo 
        FROM
          roomevents 
        WHERE RoomId = rooms.roomid 
          AND EventType = 'IN HOUSE' 
          AND EventDate = pAuditDate 
          AND TransferFlag = 0 LIMIT 1) IS NULL,  
        "OUT OF ORDER",
        rooms.cleaningstatus
      )
    ) AS cleaningstatus,
    IF(
      fGetRoomEngineeringJobDescription (pAuditDate, rooms.roomid) IS NULL,
      rooms.stateflag,
      IF(
        (SELECT
          EventNo 
        FROM
          roomevents 
        WHERE RoomId = rooms.roomid 
          AND EventType = 'IN HOUSE' 
          AND EventDate = pAuditDate 
          AND TransferFlag = 0 LIMIT 1) IS NULL, 
        "-",
        rooms.stateflag
      )
    ) AS stateFlag,
    IF(
      (SELECT
        EventNo 
      FROM
        roomevents 
      WHERE RoomId = rooms.roomid 
        AND EventType = 'IN HOUSE' 
        AND EventDate = pAuditDate 
        AND TransferFlag = 0 LIMIT 1) IS NULL,  
      fGetRoomEngineeringJobDescription (pAuditDate, rooms.roomid),
      ""
    ) AS Remarks,
    fGetRoomOccupancyStatus1 (pAuditDate, rooms.roomid) AS Status1,
    fGetRoomOccupancyStatus2 (pAuditDate, rooms.roomid) AS Status2 
  FROM
    rooms 
  WHERE rooms.stateflag <> 'DELETED' 
