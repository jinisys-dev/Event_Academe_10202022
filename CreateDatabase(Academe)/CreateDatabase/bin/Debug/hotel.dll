set 
paycash = paycash + pPayCash,
paycard = paycard + pPayCard,
paycheque = paycheque + pPayCheque,
paygiftcheque = PaygiftCheque + pPaygiftCheque,
balanceforwarded = balanceforwarded + pbalanceforwarded,
discount = discount + pDiscount,
