SELECT COUNT(*) into @cnt FROM `forex_logs`  WHERE HotelId=pHotelId AND ForExDate = pAuditDate;
if (@cnt = 0) then
select `value` as CurrencyTo, (SELECT Currency FROM currencycodes WHERE hotelid =  pHotelId AND CurrencyCode = `value`) AS currencyName from system_config where `key` = 'CURRENCY_CODE'
union
SELECT DISTINCT CurrencyTo, (SELECT Currency FROM currencycodes WHERE hotelid =  pHotelId AND CurrencyCode = CurrencyTo) AS currencyName FROM `forex_logs`  WHERE HotelId=pHotelId AND ForExDate = (SELECT ForExDate FROM `forex_logs`  ORDER BY ForExDate DESC LIMIT 1);
else
SELECT `value` AS CurrencyTo, (SELECT Currency FROM currencycodes WHERE hotelid =  pHotelId AND CurrencyCode = `value`) AS currencyName FROM system_config WHERE `key` = 'CURRENCY_CODE'
UNION
Select Distinct CurrencyTo, (select Currency from currencycodes where hotelid =  pHotelId and CurrencyCode = CurrencyTo) as currencyName from `forex_logs`  where HotelId=pHotelId and ForExDate = pAuditDate;
end if;
END */;;
