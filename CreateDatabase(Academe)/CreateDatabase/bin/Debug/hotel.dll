/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportDateRangeSalesSummary_labreza`(
    IN pHotelid INT(5),
    IN pStartDate DATE,
    IN pEndDate DATE
    )
BEGIN
    DECLARE done INT DEFAULT 0;
DECLARE currInclusion INT DEFAULT 1;
DECLARE inclusion1 VARCHAR(50) DEFAULT "";
DECLARE inclusion2 VARCHAR(50) DEFAULT "";
DECLARE inclusion3 VARCHAR(50) DEFAULT "";
DECLARE memoStr VARCHAR(30);
DECLARE cur1 CURSOR FOR SELECT DISTINCT Description FROM rateinclusions WHERE StatusFlag = 'ACTIVE' ORDER BY Description limit 3;
DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = 1;  
  OPEN cur1;
  read_loop: LOOP
    FETCH cur1 INTO memoStr;
    IF done = 1 THEN
      LEAVE read_loop;
    END IF;
    CASE currInclusion
	WHEN 1 THEN SELECT memoStr INTO inclusion1;
	WHEN 2 THEN SELECT memoStr INTO inclusion2;
	WHEN 3 THEN SELECT memoStr INTO inclusion3;
    END CASE;
    SET currInclusion = currInclusion + 1;
  END LOOP;
  CLOSE cur1;
SET @sqlString = CONCAT('SELECT transactioncode, SUM(baseamount) AS BaseAmount, SUM(grossAmount) AS GrossAmount, SUM(netAmount) AS NetAmount, SUM(netbaseamount) AS NetBaseAmount, SUM(discount) AS Discount, SUM(govttax) AS GovtTax, SUM(inclusion1) AS inclusion1, SUM(inclusion2) AS inclusion2,SUM(inclusion3) AS inclusion3, SUM(localtax) AS LocalTax, SUM(servicecharge) AS ServiceCharge, SUM(transactioncount) AS TransactionCount, TransactionDescription, acctside, SUM(monthtodate) AS MonthToDate, SUM(yeartodate) AS YearToDate FROM
(
	SELECT
	transactioncode.tranCode AS transactionCode,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', foliotransactions.baseAmount * -1, foliotransactions.baseAmount), 0)
		) AS BaseAmount,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', foliotransactions.grossAmount * -1, foliotransactions.grossAmount), 0)
		) AS GrossAmount,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', foliotransactions.netAmount * -1, foliotransactions.netAmount), 0)
		) AS NetAmount,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', foliotransactions.netBaseAmount * -1, foliotransactions.netBaseAmount), 0)
		) AS NetBaseAmount,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', foliotransactions.discount * -1, foliotransactions.discount), 0)
		) AS Discount,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', foliotransactions.governmentTax * -1, foliotransactions.governmentTax), 0)
		) AS GovtTax,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', IFNULL((SELECT foliotransactioninclusions.Amount FROM foliotransactioninclusions LEFT JOIN rateinclusions ON foliotransactioninclusions.`RateInclusionId` = rateinclusions.`RateInclusionId` WHERE FolioTransactionNo = foliotransactions.FolioTransactionNo AND rateinclusions.`Description` = ''',inclusion1,'''),0) * -1, IFNULL((SELECT foliotransactioninclusions.Amount FROM foliotransactioninclusions LEFT JOIN rateinclusions ON foliotransactioninclusions.`RateInclusionId` = rateinclusions.`RateInclusionId` WHERE FolioTransactionNo = foliotransactions.FolioTransactionNo AND rateinclusions.`Description` = ''',inclusion1,'''),0)), 0)
		) AS inclusion1,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', IFNULL((SELECT foliotransactioninclusions.Amount FROM foliotransactioninclusions LEFT JOIN rateinclusions ON foliotransactioninclusions.`RateInclusionId` = rateinclusions.`RateInclusionId` WHERE FolioTransactionNo = foliotransactions.FolioTransactionNo AND rateinclusions.`Description` = ''',inclusion2,'''),0) * -1, IFNULL((SELECT foliotransactioninclusions.Amount FROM foliotransactioninclusions LEFT JOIN rateinclusions ON foliotransactioninclusions.`RateInclusionId` = rateinclusions.`RateInclusionId` WHERE FolioTransactionNo = foliotransactions.FolioTransactionNo AND rateinclusions.`Description` = ''',inclusion2,'''),0)), 0)
		) AS inclusion2,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', IFNULL((SELECT foliotransactioninclusions.Amount FROM foliotransactioninclusions LEFT JOIN rateinclusions ON foliotransactioninclusions.`RateInclusionId` = rateinclusions.`RateInclusionId` WHERE FolioTransactionNo = foliotransactions.FolioTransactionNo AND rateinclusions.`Description` = ''',inclusion3,'''),0) * -1, IFNULL((SELECT foliotransactioninclusions.Amount FROM foliotransactioninclusions LEFT JOIN rateinclusions ON foliotransactioninclusions.`RateInclusionId` = rateinclusions.`RateInclusionId` WHERE FolioTransactionNo = foliotransactions.FolioTransactionNo AND rateinclusions.`Description` = ''',inclusion3,'''),0)), 0)
		) AS inclusion3,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', foliotransactions.localtax * -1, foliotransactions.localtax), 0)
		) AS LocalTax,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(foliotransactions.acctside = ''CREDIT'', foliotransactions.servicecharge * -1, foliotransactions.servicecharge), 0)
		) AS ServiceCharge,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',1,0)) AS TransactionCount,
	transactioncode.memo AS TransactionDescription,
	transactioncode.`AcctSide`,
	SUM(IF (DATE(foliotransactions.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''', 
		IF(foliotransactions.acctSide = ''CREDIT'', foliotransactions.netBaseAmount * -1, foliotransactions.netBaseAmount), 0)
	) AS MonthToDate,
	SUM(IF (YEAR(foliotransactions.transactiondate)=YEAR(''',pEndDate,'''), 
		IF(foliotransactions.acctSide = ''CREDIT'', foliotransactions.netBaseAmount * -1, foliotransactions.netBaseAmount), 0)
	) AS YearToDate
	FROM
	transactioncode
	LEFT JOIN
	foliotransactions
	ON
	transactioncode.tranCode = foliotransactions.transactionCode
	WHERE
	transactioncode.StateFlag = ''ACTIVE''
	AND
	foliotransactions.HotelId = ',pHotelId,'
	GROUP BY 
	transactioncode.tranCode
	UNION ALL
	SELECT
	transactioncode.tranCode AS transactionCode,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(nonguesttransaction.acctside = ''CREDIT'', nonguesttransaction.baseAmount * -1, nonguesttransaction.baseAmount), 0)
		) AS BaseAmount,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(nonguesttransaction.acctside = ''CREDIT'', nonguesttransaction.grossAmount * -1, nonguesttransaction.grossAmount), 0)
		) AS GrossAmount,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(nonguesttransaction.acctside = ''CREDIT'', nonguesttransaction.netAmount * -1, nonguesttransaction.netAmount), 0)
		) AS NetAmount,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(nonguesttransaction.acctside = ''CREDIT'', nonguesttransaction.netBaseAmount * -1, nonguesttransaction.netBaseAmount), 0)
		) AS NetBaseAmount,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(nonguesttransaction.acctside = ''CREDIT'', nonguesttransaction.discount * -1, nonguesttransaction.discount), 0)
		) AS Discount,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(nonguesttransaction.acctside = ''CREDIT'', nonguesttransaction.governmentTax * -1, nonguesttransaction.governmentTax), 0)
		) AS GovtTax,
	0 AS inclusion1,
	0 as inclusion2,
	0 as inclusion3,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(nonguesttransaction.acctside = ''CREDIT'', nonguesttransaction.localtax * -1, nonguesttransaction.localtax), 0)
		) AS LocalTax,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',
		IF(nonguesttransaction.acctside = ''CREDIT'', nonguesttransaction.servicecharge * -1, nonguesttransaction.servicecharge), 0)
		) AS ServiceCharge,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''',1,0)) AS TransactionCount,
	transactioncode.memo AS TransactionDescription,
	transactioncode.`AcctSide`,
	SUM(IF (DATE(nonguesttransaction.transactionDate) BETWEEN ''',pStartDate,''' AND ''',pEndDate,''', 
		IF(nonguesttransaction.acctSide = ''CREDIT'', nonguesttransaction.netBaseAmount * -1, nonguesttransaction.netBaseAmount), 0)
	) AS MonthToDate,
	SUM(IF (YEAR(nonguesttransaction.transactiondate)=YEAR(''',pEndDate,'''), 
		IF(nonguesttransaction.acctSide = ''CREDIT'', nonguesttransaction.netBaseAmount * -1, nonguesttransaction.netBaseAmount), 0)
	) AS YearToDate
	FROM
	transactioncode
	LEFT JOIN
	nonguesttransaction
	ON
	transactioncode.tranCode = nonguesttransaction.transactionCode
	WHERE
	transactioncode.StateFlag = ''ACTIVE''
	AND
	nonguesttransaction.HotelId = ',pHotelId,'
	GROUP BY 
	transactioncode.tranCode
) AS transactionTable GROUP BY transactioncode;'
);
PREPARE stmt FROM @sqlString;
EXECUTE stmt;
DEALLOCATE PREPARE stmt;
    END */;;
