/*!50003 SET SESSION SQL_MODE=@OLD_SQL_MODE*/;;
/*!50003 DROP PROCEDURE IF EXISTS `spGenerateDRR` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`%`*/ /*!50003 PROCEDURE `spGenerateDRR`(
	pHotelId int(4),
	pStartDate date,
	pEndDate date
)
BEGIN
	
  DECLARE done INT DEFAULT 0;
  DECLARE drrScript VARCHAR(10000);
  DECLARE drrScript2 VARCHAR(10000);
  DECLARE drrScript3 VARCHAR(10000);
  DECLARE memoStr VARCHAR(30);
  DECLARE cur1 CURSOR FOR SELECT DISTINCT(transactioncode.memo) FROM transactioncode where stateFlag = 'ACTIVE' and acctSide = 'DEBIT' and tranTypeID = 1;
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = 1;
  SET drrScript = "";
  SET drrScript2 = "";
  OPEN cur1;
  read_loop: LOOP
    FETCH cur1 into memoStr;
    IF done = 1 THEN
      LEAVE read_loop;
    END IF;
      
      select concat(drrScript, 'if(t.memo = ''',  memoStr ,  ''', if(trans.acctSide = ''DEBIT'', round(trans.netBaseAmount+trans.mealAmount,4), -round(trans.netBaseAmount+trans.mealAmount,4)), 0) AS ''',  memoStr ,  ''', '
	
	) into drrScript;	    
  END LOOP;
 
  select drrScript into drrScript2;
  select replace(drrScript2, 't.memo', 'tc.memo') into drrScript2;
  select replace(drrScript2, 'trans.netBaseAmount','ngtrans.netBaseAmount') into drrScript2;
  select replace(drrScript2, 'trans.acctSide','ngtrans.acctSide') into drrScript2;
  select replace(drrScript2, 'trans.mealAmount', '0') into drrScript2;
  select drrScript into drrScript3;
  select replace(drrScript3, 't.memo', 'tc.memo') into drrScript3;
  select replace(drrScript3, 'trans.netBaseAmount','tbl1.netBaseAmount') into drrScript3;
  select replace(drrScript3, 'trans.acctSide','tbl1.acctSide') into drrScript3;
  select replace(drrScript3, 'trans.mealAmount', '0') into drrScript3;  
  CLOSE cur1;
	  
  set @sqlString = concat(
	'(select date(trans.transactiondate) as DATE, trans.memo as MEMO, trans.transactioncode as `TRANS CODE`, trans.acctSide as `ACCT SIDE`, ', drrScript 
	,' trans.transactionSource as `TRANS SRC` '
	,' , trans.referenceno as `REF NO` '
	,' , trans.folioId as `FOLIO ID`'
	,' , if(folioschedules.`RoomType`=''FUNCTION'', '
	,' 	IF(folio.`AccountType` = ''PERSONAL'', '
	,'	(SELECT CONCAT(`firstName`," " , `lastName`) FROM guests WHERE guests.`AccountId` = folio.`CompanyId`), '
	,'	(SELECT CompanyName FROM company WHERE CompanyId = folio.`CompanyId`)), '
	,'	concat(guests.`firstName`," " , guests.`lastName`) '
	,' ) as `GUEST NAME` '
	,' , if(trans.acctSide = ''DEBIT'', trans.netBaseAmount, -trans.netBaseAmount) as `NET BASE AMT`'
	,' , if(trans.acctSide = ''DEBIT'', trans.mealAmount, -trans.mealAmount) as `MEAL AMT`'
	,' , if(trans.acctSide = ''DEBIT'', trans.localTax, -trans.localTax) as `LOC TAX`'
	,' , if(trans.acctSide = ''DEBIT'', trans.governmentTax, -trans.governmentTax) as `GOV TAX`'
	,' , if(trans.acctSide = ''DEBIT'', trans.serviceCharge, -trans.serviceCharge) as `SERV CHARGE`'
	,' , if(trans.acctSide = ''DEBIT'', trans.discount, -trans.discount) as `DISCOUNT`'
	,' , if(trans.acctSide = ''DEBIT'', round(trans.netBaseAmount+trans.mealAmount+trans.localTax+trans.governmentTax+trans.serviceCharge+trans.discount, 4), -round(trans.netBaseAmount+trans.mealAmount+trans.localTax+trans.governmentTax+trans.serviceCharge+trans.discount, 4)) as `COMPUTED REVENUE` '
	,' , if(trans.acctSide = ''DEBIT'', round(trans.baseAmount,4), -round(trans.baseAmount,4)) as `REVENUE` '
	,' , folioschedules.RoomID as `RM ID`'
	,' , folioschedules.RoomType as `RM TYPE`'
	,' , folioschedules.RATETYPE as `RATE TYPE`'
	,' , folioschedules.RATE as `RATE`'
	,' , folio.noOfAdults+folio.noOfChild as `NO OF GUEST` '	
	,' from foliotransactions trans'
	,' left join folioschedules on folioschedules.FolioId = trans.folioId '
	,' left join folio on folio.folioID = trans.folioId '
	,' left join company on company.companyid = folio.companyID '
	,' left join guests on guests.accountid = folio.accountID '
	,' left join TransactionCode t on t.tranCode = trans.transactionCode '
	,' where '
	,' date(trans.`TransactionDate`) >= date(''',pStartDate,''') and '
	,' date(trans.`TransactionDate`) <= date(''',pEndDate,''') and '
	,' trans.status = ''ACTIVE'' and trans.HotelId = ', pHotelId, ' and '
	,' t.tranTypeID = 1 order by trans.folioId) '
	,' UNION ALL '
	,' (select '
	,' date(ngtrans.transactiondate), ngtrans.memo, ngtrans.transactioncode '
	,' , ngtrans.acctSide '
	,' , ',drrScript2,' '
	,' ngtrans.transactionSource '
	,' , ngtrans.referenceno '
	,' , ngtrans.referenceFolioId as folioId '
	,' , if(ngtrans.guestName="","WALK-IN GUEST",ngtrans.guestName) as GuestName '
	,' , if(ngtrans.acctSide=''DEBIT'', ngtrans.netBaseAmount, -ngtrans.netBaseAmount) '
	,' , 0 as mealAmount '
	,' , if(ngtrans.acctSide=''DEBIT'', ngtrans.localTax, -ngtrans.localTax) '
	,' , if(ngtrans.acctSide=''DEBIT'', ngtrans.governmentTax, -ngtrans.governmentTax) '
	,' , if(ngtrans.acctSide=''DEBIT'', ngtrans.serviceCharge, -ngtrans.serviceCharge) '
	,' , if(ngtrans.acctSide=''DEBIT'', ngtrans.discount, -ngtrans.discount) '
