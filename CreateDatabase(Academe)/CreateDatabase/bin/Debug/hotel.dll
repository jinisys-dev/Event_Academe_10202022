	SUM(IF (MONTH(nonguesttransaction.transactionDate) = MONTH(''',pMonth,''') AND MONTH(nonguesttransaction.transactiondate) <= ''',pMonth,''' AND YEAR(nonguesttransaction.transactiondate)=YEAR(''',pYear,'''), 
		IF(nonguesttransaction.acctSide = "CREDIT", nonguesttransaction.netBaseAmount * -1, nonguesttransaction.netBaseAmount), 0)
	) AS MonthToDate,
	SUM(IF (YEAR(nonguesttransaction.transactiondate)=YEAR(''',pYear,''') AND YEAR(nonguesttransaction.transactiondate) <= ''',pYear,''', 
