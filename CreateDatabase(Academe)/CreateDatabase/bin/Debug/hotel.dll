/*!50003 DROP PROCEDURE IF EXISTS `spReportBInvoiceTrans` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportBInvoiceTrans`(
in pFolioId varchar(20),
in pHotelId int(5),
IN pCurrencyCode VARCHAR(10)
)
BEGIN
SELECT FolioId,
	Description,
	Memo,
	GrossAmount / rFrom * rTo AS GrossAmount,
	NetAmount / rFrom * rTo AS NetAmount,
	
	if(Percentrate > 0, (NetBase * Percentrate) / rFrom * rTo, Amountrate / rFrom * rTo) as Commission,
	(govTax + LocTax) / rFrom * rTo AS tax,
	ServiceCharge / rFrom * rTo AS ServiceCharge,
	AcctSide
FROM 
(
SELECT 
	IF(pCurrencyCode = (SELECT `value` FROM `system_config` WHERE `key` = 'CURRENCY_CODE'), 1, (SELECT RateFrom  FROM `forex_logs` WHERE HotelId = pHotelId AND ForExDate = DATE(ebi.`BookingDate`) AND CurrencyTo = pCurrencyCode)) AS rFrom,
	IF(pCurrencyCode = (SELECT `value` FROM `system_config` WHERE `key` = 'CURRENCY_CODE'), 1, (SELECT RateTo  FROM `forex_logs` WHERE HotelId = pHotelId AND ForExDate = DATE(ebi.`BookingDate`) AND CurrencyTo = pCurrencyCode)) AS rTo,
	f.`FolioId`,
	aph.Description,
	apd.Description AS Memo,
	apd.Amount AS GrossAmount,
	IF(tc.GovtTaxInclusive = 1, apd.Amount, apd.Amount +  (apd.Amount * (tc.GovtTax/100 + tc.LocalTax/100 + tc.ServiceCharge/100)))AS NetAmount, 
	if(tc.GovtTaxInclusive = 1,(apd.Amount / (1 + (tc.ServiceCharge/100) + (tc.GovtTax/100) + (tc.LocalTax/100))), apd.Amount) as NetBase,
	atl.Percentrate, 
	atl.Amountrate,
	
	IF(tc.GovtTaxInclusive = 1, (apd.Amount / (1 + (tc.ServiceCharge/100) + (tc.GovtTax/100) + (tc.LocalTax/100)) * (tc.GovtTax/100)) 
				,apd.Amount * (tc.GovtTax/100)) AS govTax,
				
	IF(tc.GovtTaxInclusive = 1, (apd.Amount / (1 + (tc.ServiceCharge/100) + (tc.GovtTax/100) + (tc.LocalTax/100)) * (tc.LocalTax/100)) 
				,apd.Amount * (tc.LocalTax/100)) AS LocTax,
	IF(tc.GovtTaxInclusive = 1, (apd.Amount / (1 + (tc.ServiceCharge/100) + (tc.GovtTax/100) + (tc.LocalTax/100)) * (tc.ServiceCharge/100)) 
				,apd.Amount * (tc.ServiceCharge/100)) AS ServiceCharge,			
	tc.AcctSide
FROM folio f 
LEFT JOIN `event_booking_info` ebi
ON f.`FolioId` = ebi.`FolioId`
LEFT JOIN `event_package_header` aph 
ON ebi.`EventPackage` = aph.`PackageId` 
LEFT JOIN `event_package_detail` apd
ON aph.PackageId = apd.PackageId
LEFT JOIN `transactioncode` tc
ON apd.TransactionCode = tc.TranCode
AND apd.Description = tc.Memo
left join `agentcommission_trans_list` atl
on tc.TranCode = atl.TranCode 
and atl.folioid = pFolioId

WHERE f.`FolioId` = pFolioId
AND f.`HotelId` = pHotelId) AS tbl1 order by AcctSide desc;
END */;;
