/*!50003 DROP PROCEDURE IF EXISTS `spGetUnPostedMeterReadings` */;;
/*!50003 SET SESSION SQL_MODE="NO_AUTO_VALUE_ON_ZERO"*/;;
/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spGetUnPostedMeterReadings`(
IN pRoomId VARCHAR(10),
in pMeterId varchar(20),
IN pDateTime DATETIME)
BEGIN
select sql1.*, (select ReadingDate from `electricityconsumptions`  WHERE `Posted` = 0 
		 AND `RoomId` = pRoomId AND `ReadingDate` >= pDateTime AND `MeterId` = pMeterId order by Id asc limit 1) bReadingDate
from
(
  SELECT 
    e.`MeterId`,
    e.`RoomId`,
    
    SUM(e.`Normal_KWH`) AS Normal_KWH,
    e.`NormalPowerRate`,
    SUM(e.NormalPowerCost) AS NormalPowerCost,
    0 AS Emergency_KWH,
    0 AS EmergencyPowerRate,
    0 AS EmergencyPowerCost  
  FROM
    `electricityconsumptions` e 
  WHERE e.`Posted` = 0 AND e.Normal_KWH > 0
  AND e.`RoomId` = pRoomId and e.`ReadingDate` > pDateTime
  and e.`MeterId` = pMeterId
  group by e.`NormalPowerRate`
  
  union all
  
    SELECT 
    e.`MeterId`,
    e.`RoomId`,
    
    0 AS Normal_KWH,
    0 as NormalPowerRate,
    0 AS NormalPowerCost,
    Sum(e.`Emergency_KWH`) AS Emergency_KWH,
    e.`EmergencyPowerRate`,
    sum(e.`EmergencyPowerCost`) AS EmergencyPowerCost  
  FROM
    `electricityconsumptions` e 
  WHERE e.`Posted` = 0 AND e.Emergency_KWH > 0
  AND e.`RoomId` = pRoomId AND e.`ReadingDate` >= pDateTime
   AND e.`MeterId` = pMeterId
  GROUP BY e.`EmergencyPowerRate`) as sql1;
END */;;
