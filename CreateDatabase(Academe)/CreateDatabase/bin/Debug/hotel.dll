/*!50003 CREATE*/ /*!50020 DEFINER=`root`@`localhost`*/ /*!50003 PROCEDURE `spReportGetFolioReservationDetails`(
IN pFolioId VARCHAR(20),
IN pHotelId INT(5)
)
BEGIN
SELECT 	f.`FolioId`,
	IF(f.`AccountType` = 'PERSONAL', IF(f.`AccountId` = '', fGetGuestName(f.`CompanyId`), fGetGuestName(f.`AccountId`))
				, fGetCompanyName(f.`CompanyId`)) AS guestName,
	f.`FromDate`,
	f.`ToDate`,
	IF(f.`FolioType` != 'GROUP', f.`NoOfAdults` + f.`NoOfChild`, (SELECT SUM(folio.`NoOfAdults` + folio.`NoOfChild`) FROM folio WHERE folio.`MasterFolio` = f.`FolioId`)) AS pax,
	(SELECT COUNT(folio.folioid) FROM folio LEFT JOIN `folioschedules` ON folio.`FolioId` = folioschedules.`FolioId` 
		WHERE f.`FolioId` = IF(f.`FolioType` != 'GROUP', folio.`FolioId`, folio.`MasterFolio`)
		AND folioschedules.`RoomId` != '' AND folioschedules.`RoomType` != 'FUNCTION') AS rooms,
	DATEDIFF(f.`ToDate`, f.`FromDate`) AS nights,
	(SELECT hotel.`CheckInTime` FROM hotel WHERE hotel.`HotelId` = pHotelId AND hotel.`StateFlag` = 'ACTIVE') AS dInTime,
	(SELECT hotel.`CheckOutTime` FROM hotel WHERE hotel.`HotelId` = pHotelId AND hotel.`StateFlag` = 'ACTIVE') AS dOutTime
	
FROM
	folio f
WHERE
	f.folioid = pFolioId;
END */;;
